{
  "name": "youtube-m3u8-proxy",
  "version": "2.0.0",
  "description": "YouTube M3U8/HLS proxy server with hybrid youtube.js + yt-dlp strategy",
  "main": "src/server-hybrid.js",
  "type": "module",
  "scripts": {
    "start": "node src/server-hybrid.js",
    "start:basic": "node src/server.js",
    "dev": "node --watch src/server-hybrid.js",
    "docker:build": "docker build -t youtube-m3u8-proxy .",
    "docker:run": "docker run -p 3000:3000 youtube-m3u8-proxy",
    "compose:up": "docker-compose up -d",
    "compose:down": "docker-compose down",
    "compose:logs": "docker-compose logs -f",
    "compose:restart": "docker-compose restart"
  },
  "keywords": [
    "youtube",
    "m3u8",
    "hls",
    "proxy",
    "streaming",
    "video",
    "youtubei.js",
    "yt-dlp",
    "hybrid"
  ],
  "author": "yunfie",
  "license": "MIT",
  "dependencies": {
    "fastify": "^5.2.1",
    "youtubei.js": "^10.5.0",
    "ioredis": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
